{
	"name":"JSONP Viewer",
	"version":"1.1.7",
	"manifest_version": 2,
	"description":"Analyse a JSON-like document in Chrome",
	"background":{
		"scripts":["background.js"]
	},
	"web_accessible_resources": ["content_style.css"],
	"browser_action":{
		"default_icon":"icon_019.png"
	},
	"icons": {
      "128": "icon_128.png",
      "16": "icon_019.png",
      "32": "icon_032.png",
      "48": "icon_048.png"
   	},
	"permissions" : [
		"tabs","http://*/*","https://*/*"
	]
}
